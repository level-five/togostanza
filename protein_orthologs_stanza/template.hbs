<!DOCTYPE html>

<html>
  <head>
    <title>Protein Orthologs</title>
    {{#each css_uri}}
    <link rel="stylesheet" href="{{this}}" />
    {{/each}}
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
  </head>

  <body>
    {{#if orthologs}}
      <table class='table'>
        <tbody>
          <tr>
            <td>
              {{#each orthologs}}
                <a href='{{protein}}' target='_blank'>{{protein_label}}</a>{{#unless is_last_data}},&nbsp;{{/unless}}
              {{/each}}
            </td>
          </tr>
        </tbody>
      </table>
    {{else}}
      <div>
        No data
      </div>
    {{/if}}
  </body>
</html>

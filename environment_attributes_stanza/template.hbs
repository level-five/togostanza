<!DOCTYPE html>

<html>
  <head>
    <title>Environment Attributes</title>
    {{#each css_uri}}
    <link rel="stylesheet" href="{{this}}" />
    {{/each}}
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
  </head>

  <body>
    {{#if environment_attr}}
      <table class='table'>
        <tbody>
          <tr>
            <th>Environment</th>
            <td>{{environment_attr.meo_label}}</td>
          </tr>
          <tr>
            <th>Description</th>
            <td>{{environment_attr.meo_description}}</td>
          </tr>
          <tr>
            <th>MEO</th>
            <td>{{environment_attr.meo_no}}</td>
          </tr>
          <tr>
            <th>Exact synonyms</th>
            <td>{{environment_attr.exact_synonyms}}</td>
          </tr>
        </tbody>
      </table>
    {{else}}
      <div>
        No data
      </div>
    {{/if}}
  </body>
</html>

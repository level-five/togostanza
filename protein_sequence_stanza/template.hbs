<!DOCTYPE html>

<html>
  <head>
    <title>Protein Sequence</title>
    {{#each css_uri}}
    <link rel="stylesheet" href="{{this}}" />
    {{/each}}
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
    {{data_download}}
  </head>

  <body>
    {{#if sequences}}
      {{#each sequences}}
        <table class='table'>
          <thead>
            <tr>
              <th>Sequence</th>
              <th>Length</th>
              <th>Mass (Da)</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>{{up_id}}</td>
              <td>{{sequence_length}}</td>
              <td>{{mass}}</td>
            </tr>
          </tbody>
        </table>
        <div>
          Last modified: {{modified}}, Version {{version}} <br />
          Checksum: {{checksum}}
          <pre>{{value}}</pre>
        </div>
      {{/each}}
    {{/if}}
  </body>
</html>


<!DOCTYPE html>

<html>
  <head>
    <title>Organism Cross References</title>
    <link rel="stylesheet" href="{{css_uri}}" />
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
  </head>

  <body>
    {{#if link_list}}
      <table class='table'>
        <tbody>
          {{#each link_list}}
            <tr>
              <th class='span2'>{{this.first.xref_db}}</th>
              <td class='span10'>
                {{#each this}}
                  <a href='{{link}}' target='_blank'>{{xref_id}}</a>{{#unless is_last_data}},&nbsp;{{/unless}}
                {{/each}}
              </td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    {{else}}
      <div>
        No items found
      </div>
    {{/if}}
  </body>
</html>


$ ->
  $('[data-stanza-type]').each ->
    $this = $(this)
    data = $this.data()

    src  = "<%= ENV['SERVER_URL'] %>/stanza/#{data.stanzaType}?gene_id=#{data.stanzaGeneId}&tax_id=#{data.stanzaTaxId}"

    iframe = $("<iframe frameborder='0' src='#{src}' width='100%' />")
    iframe.width(width) if width = data.stanzaWidth
    iframe.height(height) if height = data.stanzaHeight
    $(this).append(iframe)

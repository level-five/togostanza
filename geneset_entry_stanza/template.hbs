<!DOCTYPE html>

<html>
  <head>
    <title>Geneset Entry</title>
    {{#each css_uri}}
    <link rel="stylesheet" href="{{this}}" />
    {{/each}}
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
    {{download_csv}}
    {{download_json}}
  </head>
  <body>
    <table class='table'>
      <thead>
        <tr>
          <th>{{predicate_label}}</th>
          <th>{{object_label}}</th>
        </tr>
      </thead>
      <tbody>
        {{#each geneset_entry}}
          <tr>
            <td>{{predicate}}</td>
            <td>{{object}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </body>
</html>
